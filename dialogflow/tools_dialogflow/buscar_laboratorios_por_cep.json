{
  "openapi": "3.0.0",
  "info": {
    "title": "Dialogflow Labs Search API",
    "version": "1.0.0",
    "description": "API to search for laboratories based on CEP using Dialogflow"
  },
  "servers": [
    {
      "url": "https://dialogflow-whatsapp-bot-940683850801.southamerica-east1.run.app"
    }
  ],
  "paths": {
    "/dialogflow": {
      "post": {
        "summary": "Find laboratories by CEP",
        "operationId": "findLabsByCep",
        "requestBody": {
          "description": "Request body containing CEP information",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "cep": {
                    "type": "string",
                    "description": "CEP to search for laboratories",
                    "example": "23050410"
                  }
                },
                "required": [
                  "cep"
                ]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful response with laboratory data",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "fulfillment_response": {
                      "type": "object",
                      "properties": {
                        "messages": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "text": {
                                "type": "object",
                                "properties": {
                                  "text": {
                                    "type": "array",
                                    "items": {
                                      "type": "string"
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    },
                    "sessionInfo": {
                      "type": "object",
                      "properties": {
                        "parameters": {
                          "type": "object",
                          "properties": {
                            "labs": {
                              "type": "array",
                              "items": {
                                "type": "object",
                                "properties": {
                                  "index": {
                                    "type": "integer"
                                  },
                                  "nome": {
                                    "type": "string"
                                  },
                                  "endereco": {
                                    "type": "string"
                                  },
                                  "valor": {
                                    "type": "integer"
                                  },
                                  "distancia": {
                                    "type": "string"
                                  },
                                  "codigo": {
                                    "type": "string"
                                  }
                                }
                              }
                            },
                            "qtdeLabs": {
                              "type": "integer"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}